{
    "compilerOptions": {
        "module": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "jsx": "react",
        "target": "es6",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitThis": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules"
    ]
}